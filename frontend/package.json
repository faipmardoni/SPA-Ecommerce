{
  "name": "salestock",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=8.11.3"
  },
  "scripts": {
    "analyze": "run-s build build:with-stats start:analyzer",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > dist/client/static/bundle-stats.json",
    "lint": "run-p lint:js lint:css",
    "lint:js": "esw --ext .jsx --ext .js --color",
    "lint:css": "stylelint ./src/**/*.js",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:analyzer": "webpack-bundle-analyzer dist/client/static/bundle-stats.json",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "npm run test --updateSnapshot",
    "precommit:msg": "echo 'Linter checks...' && exit 0"
  },
  "pre-commit": [
    "precommit:msg",
    "lint"
  ],
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "dependencies": {
    "@material-ui/core": "^3.9.3",
